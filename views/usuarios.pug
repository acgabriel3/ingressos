          li.nav-item
            a.nav-link(href='/login') Settings
          li(style='float:right').nav-item
            a.nav-link(href='/') Logout    
    
    mixin img-bg(src, alt)
      img.img-bg(
        src=src,
        style=`background-image: url('${src}')`,
        alt=alt
      )&attributes(attributes)
 
    figure
      +img-bg(
        "https://www.w3schools.com/w3css/img_fjords.jpg", 
        "image"
      )(class="additional-class")
   
    table(style='width:100%', border='1')
      tr
          th CPF
          th Nome
          th E-mail
          th Senha
          th Telefone
          th Data de nascimento
          th CEP
          th Casa
      each cli in clientes      
        tr    
          td= cli.cpf
          td= cli.nome
          td= cli.email
          td= cli.senha
          td= cli.telefone
          td= cli.dt_nascimento
          td= cli.cep
          td= cli.numero_casa
          
